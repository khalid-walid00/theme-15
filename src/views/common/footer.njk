               <a aria-label="footer list" :href="item?.url" class="flex gap-3 items-center">
                  <span x-text="item.title" class=" text-base text-nowrap">
                  </span>
               </a>
            </template>
         </div>
      </div>

      <div x-show="data?.lists?.footer?.length > 0" class=" w-max flex flex-col gap-4">
